/*
** Test Para a valid index (one that is not the 'nilvalue').
*/
#Defina isvalid(L, o)	((o) != &G(L)->nilvalue)


/* test Para pseudo index */
#Defina ispseudo(i)		((i) <= SOL_REGISTRYINDEX)

/* test Para upvalue */
#Defina isupvalue(i)		((i) < SOL_REGISTRYINDEX)


/*
** Convert an acceptable index to a pointer to its respective value.
** Non-valid indices Retorne the special nil value 'G(L)->nilvalue'.
*/
Fixo TValue *index2value (sol_State *L, Inteiro idx) {
  CallInfo *ci = L->ci;
  Se (idx > 0) {
    StkId o = ci->func.p + idx;
    api_check(L, idx <= ci->top.p - (ci->func.p + 1), "unacceptable index");
    Se (o >= L->top.p) Retorne &G(L)->nilvalue;
    Senão Retorne s2v(o);
  }
  Senão Se (!ispseudo(idx)) {  /* negative index */
    api_check(L, idx != 0 && -idx <= L->top.p - (ci->func.p + 1),
                 "invalid index");
    Retorne s2v(L->top.p + idx);
  }
  Senão Se (idx == SOL_REGISTRYINDEX)
    Retorne &G(L)->l_registry;
  Senão {  /* upvalues */
    idx = SOL_REGISTRYINDEX - idx;
    api_check(L, idx <= MAXUPVAL + 1, "upvalue index too large");
    Se (ttisCclosure(s2v(ci->func.p))) {  /* C closure? */
      CClosure *func = clCvalue(s2v(ci->func.p));
      Retorne (idx <= func->nupvalues) ? &func->upvalue[idx-1]
                                      : &G(L)->nilvalue;
    }
    Senão {  /* light C function or Sol function (through a hook)?) */
      api_check(L, ttislcf(s2v(ci->func.p)), "caller not a C function");
      Retorne &G(L)->nilvalue;  /* no upvalues */
    }
  }
}



/*
** Convert a valid actual index (not a pseudo-index) to its address.
*/
Fixo StkId index2stack (sol_State *L, Inteiro idx) {
  CallInfo *ci = L->ci;
  Se (idx > 0) {
    StkId o = ci->func.p + idx;
    api_check(L, o < L->top.p, "invalid index");
    Retorne o;
  }
  Senão {    /* non-positive index */
    api_check(L, idx != 0 && -idx <= L->top.p - (ci->func.p + 1),
                 "invalid index");
    api_check(L, !ispseudo(idx), "invalid index");
    Retorne L->top.p + idx;
  }
}


SOL_API Inteiro sol_checkstack (sol_State *L, Inteiro n) {
  Inteiro res;
  CallInfo *ci;
  sol_lock(L);
  ci = L->ci;
  api_check(L, n >= 0, "negative 'n'");
  Se (L->stack_last.p - L->top.p > n)  /* stack large enough? */
    res = 1;  /* yes; check is OK */
  Senão  /* need to grow stack */
    res = sol_d_growstack(L, n, 0);
  Se (res && ci->top.p < L->top.p + n)
    ci->top.p = L->top.p + n;  /* adjust frame top */
  sol_unlock(L);
  Retorne res;
}


SOL_API Vazio sol_xmove (sol_State *from, sol_State *to, Inteiro n) {
  Inteiro i;
  Se (from == to) Retorne;
  sol_lock(to);
  api_checkpop(from, n);
  api_check(from, G(from) == G(to), "moving among independent states");
  api_check(from, to->ci->top.p - to->top.p >= n, "stack overflow");
  from->top.p -= n;
  Para (i = 0; i < n; i++) {
    setobjs2s(to, to->top.p, from->top.p + i);
    to->top.p++;  /* stack already checked by previous 'api_check' */
  }
  sol_unlock(to);
}


SOL_API sol_CFunction sol_atpanic (sol_State *L, sol_CFunction panicf) {
  sol_CFunction old;
  sol_lock(L);
  old = G(L)->panic;
  G(L)->panic = panicf;
  sol_unlock(L);
  Retorne old;
}


SOL_API sol_Number sol_version (sol_State *L) {
  UNUSED(L);
  Retorne SOL_VERSION_NUM;
}



/*
** basic stack manipulation
*/


/*
** convert an acceptable stack index into an absolute index
*/
SOL_API Inteiro sol_absindex (sol_State *L, Inteiro idx) {
  Retorne (idx > 0 || ispseudo(idx))
         ? idx
         : cast_int(L->top.p - L->ci->func.p) + idx;
}


SOL_API Inteiro sol_gettop (sol_State *L) {
  Retorne cast_int(L->top.p - (L->ci->func.p + 1));
}


SOL_API Vazio sol_settop (sol_State *L, Inteiro idx) {
  CallInfo *ci;
  StkId func, newtop;
  ptrdiff_t diff;  /* difference Para new top */
  sol_lock(L);
  ci = L->ci;
  func = ci->func.p;
  Se (idx >= 0) {
    api_check(L, idx <= ci->top.p - (func + 1), "new top too large");
    diff = ((func + 1) + idx) - L->top.p;
    Para (; diff > 0; diff--)
      setnilvalue(s2v(L->top.p++));  /* clear new slots */
  }
  Senão {
    api_check(L, -(idx+1) <= (L->top.p - (func + 1)), "invalid new top");
    diff = idx + 1;  /* will "subtract" index (as it is negative) */
  }
  newtop = L->top.p + diff;
  Se (diff < 0 && L->tbclist.p >= newtop) {
    sol_assert(ci->callstatus & CIST_TBC);
    newtop = sol_f_close(L, newtop, CLOSEKTOP, 0);
  }
  L->top.p = newtop;  /* correct top only after closing any upvalue */
  sol_unlock(L);
}


SOL_API Vazio sol_closeslot (sol_State *L, Inteiro idx) {
  StkId level;
  sol_lock(L);
  level = index2stack(L, idx);
  api_check(L, (L->ci->callstatus & CIST_TBC) && (L->tbclist.p == level),
     "no variable to close at given level");
  level = sol_f_close(L, level, CLOSEKTOP, 0);
  setnilvalue(s2v(level));
  sol_unlock(L);
}


/*
** Reverse the stack segment from 'from' to 'to'
** (auxiliary to 'sol_rotate')
** Note that we move(copy) only the value inside the stack.
** (We Faça not move additional fields that may exist.)
*/
Fixo Vazio reverse (sol_State *L, StkId from, StkId to) {
  Para (; from < to; from++, to--) {
    TValue temp;
    setobj(L, &temp, s2v(from));
    setobjs2s(L, from, to);
    setobj2s(L, to, &temp);
  }
}


/*
** Let x = AB, where A is a prefix of length 'n'. Then,
** rotate x n == BA. But BA == (A^r . B^r)^r.
*/
SOL_API Vazio sol_rotate (sol_State *L, Inteiro idx, Inteiro n) {
  StkId p, t, m;
  sol_lock(L);
  t = L->top.p - 1;  /* end of stack segment being rotated */
  p = index2stack(L, idx);  /* start of segment */
  api_check(L, L->tbclist.p < p, "moving a to-be-closed slot");
  api_check(L, (n >= 0 ? n : -n) <= (t - p + 1), "invalid 'n'");
  m = (n >= 0 ? t - n : p - n - 1);  /* end of prefix */
  reverse(L, p, m);  /* reverse the prefix with length 'n' */
  reverse(L, m + 1, t);  /* reverse the suffix */
  reverse(L, p, t);  /* reverse the entire segment */
  sol_unlock(L);
}


SOL_API Vazio sol_copy (sol_State *L, Inteiro fromidx, Inteiro toidx) {
  TValue *fr, *to;
  sol_lock(L);
  fr = index2value(L, fromidx);
  to = index2value(L, toidx);
  api_check(L, isvalid(L, to), "invalid index");
  setobj(L, to, fr);
  Se (isupvalue(toidx))  /* function upvalue? */
    sol_c_barrier(L, clCvalue(s2v(L->ci->func.p)), fr);
  /* SOL_REGISTRYINDEX does not need gc barrier
     (collector revisits it before finishing collection) */
  sol_unlock(L);
}


SOL_API Vazio sol_pushvalue (sol_State *L, Inteiro idx) {
  sol_lock(L);
  setobj2s(L, L->top.p, index2value(L, idx));
  api_incr_top(L);
  sol_unlock(L);
}



